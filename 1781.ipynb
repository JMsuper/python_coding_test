    "import heapq\n",
    "\n",
    "p_num = int(input())\n",
    "p_list = []\n",
    "for i in range(p_num):\n",
    "    p_list.append(list(map(int,input().split())))\n",
    "\n",
    "p_list.sort()\n",
    "\n",
    "queue = []\n",
    "\n",
    "for i in p_list:\n",
    "    heapq.heappush(queue,i[1])\n",
    "    if i[0] < len(queue):\n",
    "        heapq.heappop(queue)\n",
    "\n",
    "print(sum(queue))"
   ]
