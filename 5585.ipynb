   "source": [
    "price = int(input())\n",
    "money = 1000\n",
    "coin_list = [500,100,50,10,5,1]\n",
    "answer = 0\n",
    "\n",
    "remain = money - price\n",
    "i = 0\n",
    "while remain != 0:\n",
    "    coin_cnt = remain // coin_list[i]\n",
    "    answer += coin_cnt\n",
    "    remain -= coin_cnt * coin_list[i]\n",
    "    i += 1\n",
    "\n",
    "print(answer)"
   ]
  }
